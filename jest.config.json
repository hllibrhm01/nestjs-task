{
    "testEnvironment": "node",
    "testEnvironmentOptions": {
      "NODE_ENV": "test"
    },
    "restoreMocks": true,
    "coveragePathIgnorePatterns": [
      "node_modules",
      "src/config",
      "src/app.js",
      "tests",
      ".fixtures.ts$",
      "errors.ts$"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "clover",
      "html"
    ],
    "transform": {
      "^.+\\.(t|j)sx?$": "@swc/jest"
    },
    "modulePathIgnorePatterns": [
      "node_modules",
      "dist",
      "coverage",
      "src/config",
      "src/app.js"
    ]
  }
  